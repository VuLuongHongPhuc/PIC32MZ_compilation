<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background colors-->
    <Color x:Key="primaryBackColor1">#0A003A</Color>
    <Color x:Key="primaryBackColor2">#1c0E49</Color>
    <Color x:Key="secondaryBackColor1">#24105F</Color>
    
    
    <!--Windows border colors-->    
    <SolidColorBrush x:Key="winBorderColor1" Color="#ccd5f0"/>
    <SolidColorBrush x:Key="winBorderColor2" Color="#f8f9ff"/>
    
    
    <!--Color palette-->
    <SolidColorBrush x:Key="color1" Color="#E285DE"/>

    <!--Font colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD"/>
    
    <!--Panel colors-->
    <SolidColorBrush x:Key="panelColor1" Color="#200F53"/>
    <SolidColorBrush x:Key="panelColor2" Color="#FF0000"/>
    
    <!--Button colors-->
    <SolidColorBrush x:Key="buttonBorderColor" Color="#9494b8"/>
    
    <SolidColorBrush x:Key="buttonPressedColor" Color="#9494b8"/>
    

    <!--Text box colors-->
    
    <!--Chart colors-->
    <SolidColorBrush x:Key="chartColorR" Color="#FF0000"/>
    <SolidColorBrush x:Key="chartColorG" Color="#00FF00"/>
    <SolidColorBrush x:Key="chartColorB" Color="#0000ff"/>
    
    <!--Mouse colors-->
    <SolidColorBrush x:Key="mouseOverColor" Color="#bee6fd"/>
    
</ResourceDictionary>